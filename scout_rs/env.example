# Scout Client Configuration
# This file contains the environment variables needed for the Scout client
# Copy this file to .env and fill in your actual values

# Database connection (PostgREST endpoint)
SCOUT_DATABASE_REST_URL=https://your-project.supabase.co/rest/v1

# Authentication (use either SCOUT_API_KEY or SCOUT_DEVICE_API_KEY)
SCOUT_API_KEY=your_api_key_here
# OR
SCOUT_DEVICE_API_KEY=your_device_api_key_here

# Optional: Scout URL (for backward compatibility, not used in direct DB mode)
SCOUT_URL=https://your-scout-instance.com

# Optional: Logging level
RUST_LOG=info 